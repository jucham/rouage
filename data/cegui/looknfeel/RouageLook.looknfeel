    ***************************************************
    -->
    <WidgetLook name="RouageLook/Titlebar">
        <PropertyDefinition name="CaptionColour" initialValue="FFFFFFFF" redrawOnWrite="true" type="ColourRect"/>
        <Property name="NonClient" value="True" />
        <Property name="Size" value="{{0, 256}, {0, 32}}" />
        <ImagerySection name="main">
            <ImageryComponent>
                <Image name="RouageLook/NewTitlebarLeft" />
                <VertFormat type="Stretched" />
            </ImageryComponent>
            <ImageryComponent>
                <Area>
                    <Dim type="RightEdge" >
                        <OperatorDim op="Subtract">
                            <UnifiedDim scale="1" type="RightEdge"/>
                            <OperatorDim op="Add">
                                <ImageDim name="RouageLook/SysAreaRight" dimension="Width"/>
                                <ImageDim name="RouageLook/SysAreaMiddle" dimension="Width" />
                            </OperatorDim>
                        </OperatorDim>
                    </Dim>
                </Area>
                <Image name="RouageLook/NewTitlebarRight" />
                <VertFormat type="Stretched" />
                <HorzFormat type="RightAligned" />
            </ImageryComponent>
            <ImageryComponent>
                <Area>
                    <Dim type="RightEdge" >
                        <OperatorDim op="Subtract">
                            <UnifiedDim scale="1" type="RightEdge"/>
                            <ImageDim name="RouageLook/SysAreaRight" dimension="Width" />
                        </OperatorDim>
                    </Dim>
                </Area>
                <Image name="RouageLook/SysAreaMiddle" />
                <VertFormat type="Stretched" />
                <HorzFormat type="RightAligned" />
            </ImageryComponent>
            <ImageryComponent>
                <Image name="RouageLook/SysAreaRight" />
